{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["src/**/*.ts"],
      "outputLogs": "new-only",
      "outputs": ["dist/**"]
    },
    "watch": {
      "cache": true,
      "inputs": ["src/**/*.ts"],
      "outputLogs": "new-only",
      "outputs": ["dist/**"]
    }

  }
}
