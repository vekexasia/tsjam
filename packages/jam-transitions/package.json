{
  "name": "@tsjam/transitions",
  "version": "1.0.0",
  "description": "A library collecting the state transitions for JAM",
  "scripts": {
    "build": "npx rollup --config ../../build/rollup.config.mjs",
    "dev": "$npm_execpath run build -w",
    "lint": "npx eslint --ext .ts src test",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build && vitest run"
  },
  "exports": {
    ".": {
      "node": {
        "import": "./dist/node.esm.mjs",
        "default": "./dist/node.cjs.js"
      },
      "types": "./dist/types/index.d.ts"
    }
  },
  "engines": {
    "node": ">= 22.0.0"
  },
  "author": {
    "url": "https://www.andreabaccega.com/",
    "email": "vekexasia@gmail.com",
    "name": "Andrea Baccega"
  },
  "dependencies": {
    "@tsjam/codec": "*",
    "@tsjam/constants": "*",
    "@tsjam/crypto": "*",
    "@tsjam/merklization": "*",
    "@tsjam/pvm": "*",
    "@tsjam/types": "*",
    "@tsjam/utils": "*",
    "@tsjam/serviceaccounts": "*",
    "neverthrow": "^8.1.1"
  },
  "license": "MIT"
}
