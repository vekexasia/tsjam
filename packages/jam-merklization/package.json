{
  "name": "@vekexasia/jam-merklization",
  "version": "1.0.0",
  "description": "MMR and merkle trees used in JAM",
  "scripts": {
    "build": "rollup --config ../../build/rollup.config.mjs",
    "dev": "$npm_execpath run build -w",
    "lint": "npx eslint --ext .ts src test",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build && vitest run"
