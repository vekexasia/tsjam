  "exports": {
    ".": {
      "node": {
        "import": "./dist/node.esm.mjs",
        "default": "./dist/node.cjs.js"
      },
      "types": "./dist/types/index.d.ts"
    }
  },
  "engines": {
    "node": ">= 22.0.0"
  },
  "author": {
    "url": "https://www.andreabaccega.com/",
    "email": "vekexasia@gmail.com",
    "name": "Andrea Baccega"
  },
  "dependencies": {
    "@vekexasia/jam-codec": "*",
    "@vekexasia/jam-crypto": "*",
    "@vekexasia/jam-types": "*",
    "@vekexasia/jam-utils": "*"
  },
  "license": "MIT"
}
