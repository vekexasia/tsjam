{
  "name": "@tsjam/utils",
  "version": "1.0.0",
  "description": "Utility functions for JAM",
  "dependencies": {
    "@tsjam/constants": "workspace:*",
    "@tsjam/types": "workspace:*",
    "@vekexasia/bigint-uint8array": "^1.1.1",
    "bigint-buffer": "^1.1.5",
    "neverthrow-safe-ret": "^1.0.1",
