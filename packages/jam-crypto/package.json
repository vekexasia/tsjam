{
  "name": "@vekexasia/jam-crypto",
  "version": "1.0.0",
  "description": "Crypto utils for JAM",
  "scripts": {
    "build": "rm -rf dist/; rollup --config rollup.config.mjs",
    "lint": "npx eslint --ext .ts src test",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build && vitest run"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "node": {
        "import": "./dist/node.esm.mjs",
        "default": "./dist/node.cjs.js"
      }
    }
  },
  "engines": {
    "node": ">= 22.0.0"
  },
  "author": {
    "url": "https://www.andreabaccega.com/",
    "email": "vekexasia@gmail.com",
    "name": "Andrea Baccega"
  },
  "license": "MIT",
  "dependencies": {
    "blake2b-wasm": "^2.4.0",
    "fast-shuffle": "^6.1.0",
    "keccak256": "^1.0.6",
    "sodium-native": "^4.1.1"
  },
  "devDependencies": {
    "@types/blake2b-wasm": "^2",
    "@types/sodium-native": "^2.3.9"
  }
}
