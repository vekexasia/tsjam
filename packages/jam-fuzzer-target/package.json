{
  "name": "@tsjam/fuzzer-target",
  "version": "0.7.1",
  "description": "Fuzzer Target for conformance Test for JAM",
  "dependencies": {
    "@tsjam/codec": "*",
    "@tsjam/constants": "*",
    "@tsjam/core": "*",
    "neverthrow": "^8.2.0",
    "nexe": "^5.0.0-beta.4",
    "type-fest": "^4.41.0"
  },
  "scripts": {
    "build": "npx rollup --config ./rollup.config.mjs",
    "build:nexe": "npx nexe -i dist/cli.mjs -o jam-fuzzer-target --build --verbose --target=22.18.0 --make=-j8 --flags=\"--max-old-space-size=16384\"",
    "build:deliverable": "yarn build && node scripts/build-deliverable.js",
    "lint": "npx eslint --ext .ts src test",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build && vitest run"
  },
  "exports": {
    ".": {
      "node": {
        "import": "./dist/node.esm.mjs",
        "default": "./dist/node.cjs.js"
      },
      "types": "./dist/types/index.d.ts"
    }
  },
  "engines": {
    "node": ">= 22.0.0"
  },
  "author": {
    "url": "https://www.andreabaccega.com/",
    "email": "vekexasia@gmail.com",
    "name": "Andrea Baccega"
  },
  "license": "MIT",
  "jam:protocolVersion": "0.7.1"
}
