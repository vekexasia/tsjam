{
  "name": "@tsjam/codec",
  "version": "0.7.1",
  "description": "A library to convert back and forth according appendix C of graypaper. Also called SCALE",
  "scripts": {
    "build:old": "rm -rf dist; npx rollup --config ../../build/rollup.config.mjs",
    "build:new": "rm -rf dist; npx tsgo; npx tsc-alias",
    "build": "yarn run build:old",
    "dev": "$npm_execpath run build -w",
    "lint": "npx eslint --ext .ts src test",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build && vitest run"
  },
  "exports": {
    ".": {
      "node": {
        "default": "./dist/index.js"
      },
      "types": "./dist/types/index.d.ts"
    },
    "./test/*": "./test/*"
  },
  "engines": {
    "node": ">= 22.0.0"
  },
  "author": {
    "url": "https://www.andreabaccega.com/",
    "email": "vekexasia@gmail.com",
    "name": "Andrea Baccega"
  },
  "dependencies": {
    "@tsjam/constants": "*",
    "@tsjam/crypto": "*",
    "@tsjam/types": "*",
    "@tsjam/utils": "*",
    "uint8array-extras": "^1.4.0"
  },
  "license": "MIT",
  "jam:protocolVersion": "0.7.1"
}
